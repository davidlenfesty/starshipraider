{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Host Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./build/firmware.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": ["./f103_openocd.cfg"],
            "device": "STM32F103CC",
            "demangle": true,
            "swoConfig": {
                "enabled": true,
                "source": "probe",
                "cpuFrequency": 8000000,
                "swoFrequency": 64000,
                "decoders": [
                    {
                        "port": 0,
                        "label": "Debug out",
                        "type":"console",
                    }
                ]
            }
        },
        {
            "name": "Probe Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./probe_build/firmware.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": ["./f103_openocd.cfg"],
            "device": "STM32F103CC",
            "demangle": true
        }
    ]
}